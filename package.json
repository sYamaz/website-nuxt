{
  "name": "website-nuxt",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "prepare": "nuxi prepare",
    "front": "cross-env NODE_ENV=\"local_front\" nuxt",
    "dev": "cross-env NODE_ENV=\"local\" nuxi dev",
    "build": "cross-env NODE_ENV=\"production\" nuxi build",
    "start": "nuxi preview",
    "generate": "nuxi generate",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .eslintignore .",
    "lint": "npm run lint:js",
    "lintfix": "npm run lint:js -- --fix",
    "build:gh-pages": "cross-env NODE_ENV=\"production\" nuxi build",
    "generate:gh-pages": "cross-env NODE_ENV=\"production\" nuxi generate",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "update:lintignore": "echo autogenerated/ > .eslintignore && cat .gitignore >> .eslintignore",
    "update:articles": "python3 -B scripts/articles_generator.py autogenerated/articles.ts"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "core-js": "^3.19.3",
    "cross-env": "^7.0.3",
    "nuxt-edge": "^2.16.0-27720022.54e852f",
    "vue": "^2.6.14",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "vuetify": "^2.6.1",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@nuxt/bridge": "npm:@nuxt/bridge-edge",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/vuetify": "^1.12.3",
    "eslint": "^8.4.1",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-vue": "^8.2.0",
    "push-dir": "^0.4.1"
  }
}
